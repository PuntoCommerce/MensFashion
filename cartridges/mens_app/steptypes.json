            },
            {
              "@name": "siteNameURL",
              "@type": "string",
              "@required": "true",
              "description": "using images public path for saving five, this use file.CATALOG class and need the store site name for correct working, the site name can be find in administration > development setup > catalog > [name of catalog]",
