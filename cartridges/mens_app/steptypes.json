            },
            {
              "@name": "pricebookListID",
              "@type": "string",
              "@required": "true",
              "description": "the id of the pricebook can be find in the pricing section of a product",
              "@trim": true
            },
            {
              "@name": "useDescription",
              "@type": "boolean",
              "@required": "true",
              "description": "use Short Descrption or not, if using html in description field may fail at creating xml",
              "@trim": true
            },
            {
              "@name": "fileName",
              "@type": "string",
              "@required": "true",
              "description": "Final name for file path",
