<footer id="footercontent">
    <div class="container">
        <div class="footer-container row footer-row-custom">
            <div class="footer-item col-sm-3 store custom-footer-item">
                <img class="hidden-md-down" id="logoCustom" src="${URLUtils.staticURL('/images/foot1.png')}"
                    alt="${Resource.msg('global.storename', 'common', null)}" />
                <img class="d-lg-none" src="${URLUtils.staticURL('/images/foot1.png')}"
                    alt="${Resource.msg('global.storename', 'common', null)}" />
            </div>
            <div class="footer-item col-sm-3 store custom-footer-item">
                <img class="hidden-md-down" id="logoCustom" src="${URLUtils.staticURL('/images/foot2.png')}"
                    alt="${Resource.msg('global.storename', 'common', null)}" />
                <img class="d-lg-none" src="${URLUtils.staticURL('/images/foot2.png')}"
                    alt="${Resource.msg('global.storename', 'common', null)}" />
            </div>
            <div class="footer-item col-sm-6 store footer-sociales custom-footer-item">
                <a class="logo-sociales" href="${URLUtils.url('Home-Show')}" title="${Resource.msgf('global.homepage.tooltip', 'common', null, Resource.msg('global.storename', 'common', null), Resource.msg(
                                                                                    'global.home', 'common', null))}">
                    <img class="hidden-md-down" id="logoCustom" src="${URLUtils.staticURL('/images/footsoc.png')}"
                        alt="${Resource.msg('global.storename', 'common', null)}" />
                    <img class="d-lg-none" src="${URLUtils.staticURL('/images/footsoc.png')}"
                        alt="${Resource.msg('global.storename', 'common', null)}" />
                </a>
            </div>
        </div>
        <div class="footer-container row">
            <div class="footer-item col-sm-2 collapsible-xs">
                <iscontentasset aid="footer-about" />
            </div>
            <div class="footer-item col-sm-2 collapsible-xs">
                <iscontentasset aid="footer-account" />
            </div>
            <div class="footer-item col-sm-2 collapsible-xs">
                <iscontentasset aid="footer-support" />
            </div>
            <div class="footer-item col-sm-2 store">
                <iscontentasset aid="footer-locate-store" />
            </div>
        </div>
        <div class="row-btn-back-top">
            <isslot id="footer-column-m" description="Content in column 1 of the Footer" context="global" />
            <button class="back-to-top" title="${Resource.msg('button.back_to_top', 'common', null)}"
                aria-label="${Resource.msg('button.back_to_top', 'common', null)}">
                <span class="fa-stack fa-lg" aria-hidden="true">
                    <i class="fa fa-circle fa-inverse fa-stack-2x"></i>
                    <i class="fa fa-arrow-up fa-stack-1x"></i>
                </span>
            </button>
        </div>
    </div>
</footer>