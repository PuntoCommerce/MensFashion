<isdecorate template="common/layout/page">
    <isscript>
        var assets = require('*/cartridge/scripts/assets.js');
        assets.addCss('/css/wishlist.css');
        assets.addJs('/js/wishlist.js');
         assets.addJs('/js/productDetail.js');
        assets.addJs('/js/customWishlist.js');
        assets.addCss('/css/login.css');
    </isscript>
    <div class="cart-bg">
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-12">
                    <div class="carrito">
                        <h1>${Resource.msg('wishlist.hero.image.header', 'wishlist', null)}</h1>
                        <!--<p class="d-none d-xs-none d-sm-block d-md-block d-lg-block">Â¿Necesitas ayuda? Marca: 1-800-555-0199</p> -->
                    </div>
                </div>
            </div>
        </div>

    </div>
    <div class="container-fluid">
        <!---Breadcrumbs--->
        <isinclude template="components/breadcrumbs/pageBreadcrumbs" />
        <isif condition="${pdict.privateList}">
            <div class="row">
                <div class="col-12 text-center">
                    <p>${pdict.errorMsg}</p>
                </div>
            </div>
            <iselse>
                <isinclude template="wishlist/components/wishlist" />
        </isif>
    </div>
</isdecorate>